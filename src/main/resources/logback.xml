<configuration>
    <appender
        name="STDOUT"
        class="ch.qos.logback.core.ConsoleAppender" >
        <encoder>
            <pattern>[%level] %msg%n</pattern>
        </encoder>
    </appender>

    <root level="OFF" >
        <appender-ref ref="STDOUT" />
    </root>
</configuration>